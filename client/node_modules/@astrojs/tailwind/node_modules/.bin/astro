#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/mnt/c/Proyectos/MVA-FOOD/client/node_modules/.pnpm/astro@5.9.2_@types+node@22._ad5bd7fd0db018969ac1f6ac1a2921a1/node_modules/astro/node_modules:/mnt/c/Proyectos/MVA-FOOD/client/node_modules/.pnpm/astro@5.9.2_@types+node@22._ad5bd7fd0db018969ac1f6ac1a2921a1/node_modules:/mnt/c/Proyectos/MVA-FOOD/client/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/mnt/c/Proyectos/MVA-FOOD/client/node_modules/.pnpm/astro@5.9.2_@types+node@22._ad5bd7fd0db018969ac1f6ac1a2921a1/node_modules/astro/node_modules:/mnt/c/Proyectos/MVA-FOOD/client/node_modules/.pnpm/astro@5.9.2_@types+node@22._ad5bd7fd0db018969ac1f6ac1a2921a1/node_modules:/mnt/c/Proyectos/MVA-FOOD/client/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../../astro@5.9.2_@types+node@22._ad5bd7fd0db018969ac1f6ac1a2921a1/node_modules/astro/astro.js" "$@"
else
  exec node  "$basedir/../../../../../../astro@5.9.2_@types+node@22._ad5bd7fd0db018969ac1f6ac1a2921a1/node_modules/astro/astro.js" "$@"
fi
