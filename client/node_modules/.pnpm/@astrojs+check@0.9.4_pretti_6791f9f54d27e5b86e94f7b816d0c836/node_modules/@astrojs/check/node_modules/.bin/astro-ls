#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/mnt/c/Proyectos/MVA-FOOD/client/node_modules/.pnpm/@astrojs+language-server@2._21eff626d4c65c3d7c14dd77f4cf0e05/node_modules/@astrojs/language-server/bin/node_modules:/mnt/c/Proyectos/MVA-FOOD/client/node_modules/.pnpm/@astrojs+language-server@2._21eff626d4c65c3d7c14dd77f4cf0e05/node_modules/@astrojs/language-server/node_modules:/mnt/c/Proyectos/MVA-FOOD/client/node_modules/.pnpm/@astrojs+language-server@2._21eff626d4c65c3d7c14dd77f4cf0e05/node_modules/@astrojs/node_modules:/mnt/c/Proyectos/MVA-FOOD/client/node_modules/.pnpm/@astrojs+language-server@2._21eff626d4c65c3d7c14dd77f4cf0e05/node_modules:/mnt/c/Proyectos/MVA-FOOD/client/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/mnt/c/Proyectos/MVA-FOOD/client/node_modules/.pnpm/@astrojs+language-server@2._21eff626d4c65c3d7c14dd77f4cf0e05/node_modules/@astrojs/language-server/bin/node_modules:/mnt/c/Proyectos/MVA-FOOD/client/node_modules/.pnpm/@astrojs+language-server@2._21eff626d4c65c3d7c14dd77f4cf0e05/node_modules/@astrojs/language-server/node_modules:/mnt/c/Proyectos/MVA-FOOD/client/node_modules/.pnpm/@astrojs+language-server@2._21eff626d4c65c3d7c14dd77f4cf0e05/node_modules/@astrojs/node_modules:/mnt/c/Proyectos/MVA-FOOD/client/node_modules/.pnpm/@astrojs+language-server@2._21eff626d4c65c3d7c14dd77f4cf0e05/node_modules:/mnt/c/Proyectos/MVA-FOOD/client/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../../@astrojs+language-server@2._21eff626d4c65c3d7c14dd77f4cf0e05/node_modules/@astrojs/language-server/bin/nodeServer.js" "$@"
else
  exec node  "$basedir/../../../../../../@astrojs+language-server@2._21eff626d4c65c3d7c14dd77f4cf0e05/node_modules/@astrojs/language-server/bin/nodeServer.js" "$@"
fi
